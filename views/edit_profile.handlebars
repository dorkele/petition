<div class="container">
  <a class="top-left" href="/">Petition</a>
<a class="top-right" href="/logout">Log Out</a>
<a class="top-right-middle" href="/signers">Signers</a>
  <div class="left">
<div id="edit">Edit your profile:</div>
{{#if error}} <div class="error">O-ou, something went wrong. We are very sorry, please try again!</div> {{/if}}
</div>
<form class="right" method="POST">
    <div class="inputs">
    <p>First</p>
    <input type="text" id="first" name="first" value={{first}}>
    </div>
    <div class="inputs">
    <p>Last</p>
    <input type="text" id="last" name="last" value={{last}}>
    </div>
    <div class="inputs">
    <p>Email</p>
    <input type="text" id="email" name="email" value={{email}}>
    </div>
    <div class="inputs">
     <p>New password</p>
    <input type="password" id="password" name="password" placeholder="Change password">
    </div>
    <div class="inputs">
    <p>Age</p>
    <input type="text" id="age" name="age" value={{age}}>
    </div>
    <div class="inputs">
    <p>City</p>
    <input type="text" id="city" name="city" value={{city}}>
    </div>
    <div class="inputs">
    <p>Website</p>
    <input type="text" id="url" name="url" value={{url}}>
   </div>
    <div class="button"><button id="button">UPDATE</button></div>
     

      <input type="hidden" name="_csrf" value="{{csrfToken}}">
 
</form>
</div>